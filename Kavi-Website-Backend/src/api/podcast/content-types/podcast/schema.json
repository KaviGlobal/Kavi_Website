{
  "kind": "collectionType",
  "collectionName": "podcasts",
  "info": {
    "singularName": "podcast",
    "pluralName": "podcasts",
    "displayName": "Podcast"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "PreviewContent": {
      "type": "richtext"
    },
    "Thumbnail": {
      "type": "component",
      "repeatable": false,
      "component": "media.image"
    },
    "FullContent": {
      "type": "richtext"
    },
    "CoverImage": {
      "type": "component",
      "repeatable": false,
      "component": "media.image"
    },
    "Media": {
      "type": "component",
      "repeatable": true,
      "component": "media.image"
    },
    "DataReleased": {
      "type": "date"
    },
    "Tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "Authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::person.person"
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "Speakers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::person.person"
    }
  }
}
