{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "CoverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "ShortContent": {
      "type": "richtext"
    },
    "Media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "PublishedOn": {
      "type": "date"
    },
    "ExpiresOn": {
      "type": "date"
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "blog",
        "news",
        "usecase",
        "success_story",
        "publications",
        "conferences",
        "white_papers",
        "magazines",
        "podcasts_talks",
        "whitepaper"
      ]
    },
    "Summary": {
      "type": "text"
    },
    "FullContent": {
      "type": "richtext"
    },
    "Tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "Authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::author.author"
    }
  }
}
